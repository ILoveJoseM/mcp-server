{
    "name": "jose-chan/laravel-mcp-server",
    "description": "A mcp server for laravel",
    "keywords": ["laravel", "extension", "mcp"],
    "authors": [{
        "name": "JoseChan",
        "email": "524233828@qq.com"
    }],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^7.1.3",
        "laravel/framework": "^5.6",
        "symfony/yaml": "^5.3"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "JoseChan\\McpServer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ],
            "providers": [
                "JoseChan\\McpServer\\Providers\\McpServerServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
